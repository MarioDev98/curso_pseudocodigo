Proceso FraseySumaNumerosEnteros
		definir nombre1 Como Caracter;
		definir numero1,numero2,sumafinal Como Entero;
		escribir "Ingresa tu primer nombre";
		leer nombre1;
		escribir "Ingresa un número entero ";
		leer numero1;
		escribir "ingresa otro número entero";
		leer numero2;	
		sumafinal<-numero1+numero2;
		escribir nombre1, "la suma es " , sumafinal;
FinProceso
