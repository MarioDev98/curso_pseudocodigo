Proceso NumerosDelUnoAlDiez
	//Instrucciones
	//Quiero que con el ciclo mientras muestren los numeros de 1 en 1 hasta el 10
	definir contador Como Entero;
	//contador empieza en 1
	contador <- 1 ;
	Mientras contador <= 10  Hacer
		imprimir "Numero ", contador;
		contador <- contador + 1;
	FinMientras	
FinProceso
